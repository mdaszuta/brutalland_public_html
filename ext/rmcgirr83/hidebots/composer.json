{
    "name": "rmcgirr83/hidebots",
    "type": "phpbb-extension",
    "description": "Hides robots in the Who is online stats to all but those who are administrators",
    "version": "1.0.6",
    "time": "2020-09-12",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpbb/phpbb": ">=3.2",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Hide Bots",
        "soft-require": {
            "phpbb/phpbb": ">=3.2"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/hide_bots",
            "filename": "version_check",
            "ssl": true
        }
    }
}