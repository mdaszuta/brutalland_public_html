{
    "name": "rmcgirr83/searchusertopics",
    "type": "phpbb-extension",
    "description": "This Extension displays number of topics started by  auser when visiting that users profile",
    "version": "1.0.1",
    "time": "2017-01-09",
    "keywords": [
        "phpbb",
        "extension",
        "topics",
        "users",
        "profile"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.1,<3.3"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Search user topics",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.1,<3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/phpbb_3.1_search_user_topics",
            "filename": "version_check"
        }
    }
}