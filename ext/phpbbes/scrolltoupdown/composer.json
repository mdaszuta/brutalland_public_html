{
    "name": "phpbbes/scrolltoupdown",
    "type": "phpbb-extension",
    "description": "A phpBB 3.1 extension that adds a Scroll to Up and Bottom button making it easy to return to the up and bottom of a page.",
    "homepage": "www.phpbb-es.com",
    "version": "1.0.2",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Raul Arroyo",
            "username": "Raul [ThE KuKa]",
            "email": "thekuka@phpbb-es.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0",
        "phpbb/phpbb": ">=3.2.0"
    },
    "extra": {
        "display-name": "Scroll To Up and Bottom",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/scroll_to_up_and_bottom",
            "filename": "version_check"
        }
    }
}