{
	"name": "lotusjeff/sitemap",
	"type": "phpbb-extension",
	"description": "SEO Sitemap creates the XML sitemap index and listings",
	"homepage": "https://github.com/LotusJeff/sitemap",
	"version": "0.2.0",
	"keywords": ["phpbb", "extension", "seo", "sitemap"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Jeff Cocking (LotusJeff)",
			"email": "jeff.cocking@gmail.com",
			"homepage": "http://www.cocking.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master",
		"squizlabs/php_codesniffer": "2.*"
	},
	"extra": {
		"display-name": "SEO Sitemap",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0-RC2,<3.2@dev"
		},
		"version-check": {
			"host": "www.cocking.com",
			"directory": "/phpbb-ext/sitemap",
			"filename": "version-check.html",
			"ssl": "true"
		}
	}
}